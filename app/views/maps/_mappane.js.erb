$('#knitter-map-pane').css('height', $(this).height() * .9 );
$('#knitter-map-pane').css('display', 'block');

setTimeout(function(){

	var map
	(function(){
	  window.mapKnitter = new MapKnitter.Map({
	    latlng:     L.latLng(<%= @map.lat %>, <%= @map.lon %>),
	    zoom:       <%= @map.zoom %>,
	    readOnly:   <%= @embed == true %>,
	    warpablesUrl:   "<%= url_for([@map, :warpables])+'.json' unless @map.warpables.empty? %>"
	  });
	 })();

}, 1000);