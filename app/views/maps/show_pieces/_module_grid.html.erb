              <%= render partial: "maps/show_pieces/module_grid_photo_half", :locals => { :grid => grid, :photo => grid.photos[0][0], :float => 'left', :height => 'full' } %>
             
              <%= render partial: "maps/show_pieces/module_grid_photo_half", :locals => { :grid => grid, :photo => grid.photos[1][0], :float => 'right', :height => 'full' } %>

