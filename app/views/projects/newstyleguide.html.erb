<div id='project-creation' class='row group'>
	<section id='photo-manager' class='side-nav four columns'>	
			<nav id='navbar-create'>
				<nav class='main-nav navbar padding-top'>
					<div class='row group wrapper'>
						<h1 class='logo five columns first-header h5'>
							<a href='/'>
								<i class='fa fa-paper-plane'></i>
								Whirld
							</a>
						</h1>
						<a class='toggle seven columns first-header' href='#'>
							<i class='fa fa-bars h5 pull-right'></i>
						</a>
					</div>
				</nav>

				<nav id='photo-state' class='tab-nav row group padding-top'>
					<div class='tab six columns h-centered' data-photo-state='uploaded' >
						<a href='#' class='active'>
							<i class='fa fa-upload'></i>
							Uploaded
						</a>
					</div>
					<div class='tab six columns h-centered' data-photo-state='saved'>
						<a href='#' class='inactive'>
							<i class='fa fa-save'></i>
							Saved
						</a>
					</div>
				</nav>

				<nav id='photo-cat-tags' class='row group padding-bottom padding-top'>
					<ul class='tag-list inline-list twelve columns font_small wrapper'>
						<li class='tag item'><a href='#' class='active'>Aerial</a></li>
						<li class='tag item'><a href='#' class='inactive'>Street View</a></li>
						<li class='tag item'><a href='#' class='inactive'>People</a></li>
					</ul>
				</nav>
			</nav>

			<section id='uploaded' class='photo-list first-section scrollable'>
				<div class='photo-row row group wrapper'>
					
					<article id='photo-upload' class='preview img-container grabbable h-centered six columns'>
						<i class='fa fa-plus-circle v-centered h1-size'></i>
					</article>

					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("square.png", :alt => "") %></div>
						</div>
					</article>

				</div> <!-- end row group -->
				
				<div class='photo-row row group wrapper'>
					<article class='preview-chosen preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<i class='fa fa-check-circle h1-size icon-overlay'></i>
							<div class='img-container'><%= image_tag("vertical.png", :alt => "") %></div>
						</div>
					</article>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("comparison-bar.png", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->

				<div class='photo-row row group wrapper'>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("square.png", :alt => "") %></div>
						</div>
					</article>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("vertical.png", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->

				<div class='photo-row row group wrapper'>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("project-chosen01.jpg", :alt => "") %></div>
						</div>
					</article>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("project-chosen02.jpg", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->
			</section>

			<section id='saved' class='photo-list first-section scrollable hidden'>
				<div class='photo-row row group wrapper'>
					
					<article id='photo-upload' class='preview img-container grabbable h-centered six columns'>
						<i class='fa fa-plus-circle v-centered h1-size'></i>
					</article>

					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("comparison-bar.png", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->

				<div class='photo-row row group wrapper'>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("square.png", :alt => "") %></div>
						</div>
					</article>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("vertical.png", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->

				<div class='photo-row row group wrapper'>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("project-chosen01.jpg", :alt => "") %></div>
						</div>
					</article>
					<article class='preview img-container grabbable six columns h-centered'>
						<div class='img-wrapper v-centered'>
							<div class='img-container'><%= image_tag("project-chosen02.jpg", :alt => "") %></div>
						</div>
					</article>
				</div> <!-- end row group -->
			</section>

	</section>

	<div class='eight columns'>
		<section id='project-header'>
			<div class='wrapper'>
				<form action=''>
					<div class='row group'>
						<div class='twelve columns'>
							<h1 class='project-title first-header h-centered'>
								<textarea name='project-title' placeholder='Title'></textarea>
							</h1>
						</div>
					</div>
					<div class='row group'>
						<a class='user-meta eight columns' href='/'>
							<div class='image profile-thumb'>
								
							</div>
							<span class='user-meta h3-size'>Juan Doe, Drone Pilot</span>
						</a>
						<div class='four columns'>
							<input class='user-meta h3-size' placeholder='Location'>
						</div>
					</div>
					<div class='row group'>
						<p class='project-description'>
								<textarea name='project-description' placeholder='Describe your project.'></textarea>
						</p>
					</div>
					<div class='row group'>
						<div class='twelve columns'>
								<ul class='tag-list inline-list padding-top font_small'>
									<li class='project-tags tag item'>
										<textarea id='project-tags' name='project-tags' placeholder='Tag your project.'></textarea>
									</li>
									<li class='tag item'>
										<a href='/'>california</a>
										<i class='fa fa-remove'></i>
									</li>
									<li class='tag item'>
										<a href='/'>drought</a>
										<i class='fa fa-remove'></i>
									</li>
									
								</ul>
						</div>
					</div>
				</form>
			</div>
		</section>

		<section id='project-basemap' class=''>
		</section>

		<ul class='button-bar inline-list'>
			<li class='option hidden item' data-module='photo'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-file-photo-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Photo</span>
			</li>
			<li class='option hidden item' data-module='grid'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-server'></i>
				</button><br>
				<span class='invisible button-label font_small'>Grid</span>
			</li>
			<li class='option hidden item' data-module='comparison'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-server'></i>
				</button><br>
				<span class='invisible button-label font_small'>Compare</span>
			</li>
			<li class='item option-toggle'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-plus'></i>
				</button><br>
				<span class='invisible font_small'>Add</span>
			</li>
			<li class='option hidden item' data-module='half'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-file-server-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Half</span>
			</li>
			<li class='option hidden item' data-module='text'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-file-text-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Text</span>
			</li>
			<li class='option hidden item' data-module='video'>
				<button class='add-module-end'>
					<i class='h2-size fa fa-file-video-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Video</span>
			</li>
		</ul>
		
		<section class='text-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-file-text'></i>
					<span class='cursor-def'>Text</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<textarea name='text-module-body' placeholder='Add some text' class='twelve columns'></textarea>
			</div>
		</section>

		<div class='button-bar button-bar-btw'>
			<button class='add-module-btw add-module'>
				<i class='fa fa-plus font_small'></i>
			</button>
		</div>

		<section class='video-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-file-video-o'></i>
					<span class='cursor-def'>Video</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<textarea class='padding-bottom' name='text-module-body' placeholder='Insert video URL from Youtube or Vimeo' class='twelve columns'></textarea>
			</div>
		</section>

		<div class='button-bar button-bar-btw'>
			<button class='add-module-btw add-module'>
				<i class='fa fa-plus font_small'></i>
			</button>
		</div>

		<section class='photo-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-file-photo-o'></i>
					<span class='cursor-def'>Photo</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<div class='droppable dropzone h-centered'>
					<p class='caps'>Drag photo here.</p>
				</div>
			</div>
		</section>

		<div class='button-bar button-bar-btw'>
			<button class='add-module-btw add-module'>
				<i class='fa fa-plus font_small'></i>
			</button>
		</div>

		<section class='photo-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-file-photo-o'></i>
					<span class='cursor-def'>Photo</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<div class='dropped dropzone h-centered'>
					<p class='caps'>Pretend a photo is about to be dropped</p>
				</div>
			</div>
		</section>

		<section class='photo-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-file-photo-o'></i>
					<span class='cursor-def'>Photo</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<article class='dropped-photo twelve columns'>
					<button class='remove-photo hidden'>
						<i class='fa fa-remove'></i>
					</button>
					<div class='img-container'><%= image_tag("project-chosen02.jpg", :alt => "") %></div>
					<textarea name='photo-caption' class='caption h-centered' placeholder='Add optional caption'></textarea>
					</input>
				</article>
			</div>
		</section>

		<div class='button-bar button-bar-btw'>
			<button class='add-module-btw add-module'>
				<i class='fa fa-plus font_small'></i>
			</button>
		</div>

		<section class='half-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-server'></i>
					<span class='cursor-def'>Half & Half</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<div class='text-module h-centered six columns'>
					<textarea name='text-module-body' placeholder='Add some text' class='twelve columns'></textarea>
				</div>
				<div class='droppable dropzone h-centered six columns'>
					<p class='caps'>A photo could be dropped here.</p>
				</div>
			</div>
		</section>

		<div class='button-bar button-bar-btw'>
			<button class='add-module-btw add-module'>
				<i class='fa fa-plus font_small'></i>
			</button>
		</div>

		<section class='comparison-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-server'></i>
					<span class='cursor-def'>Pete's Comparison bar</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>
			<div class='row group wrapper'>
				<div class='droppable dropzone h-centered six columns'>
					<p class='caps'>Choose two photos to compare side-by-side.</p>
				</div>
			</div>
		</section>

		<ul class='button-bar button-bar-btw inline-list'>
			<li class='option hidden item' data-module='photo'>
				<button class='add-module-btw'>
					<i class='fa fa-file-photo-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Photo</span>
			</li>
			<li class='option hidden item' data-module='grid'>
				<button class='add-module-btw'>
					<i class='fa fa-server'></i>
				</button><br>
				<span class='invisible button-label font_small'>Grid</span>
			</li>
			<li class='option hidden item' data-module='comparison'>
				<button class='add-module-btw'>
					<i class='fa fa-server'></i>
				</button><br>
				<span class='invisible button-label font_small'>Compare</span>
			</li>
			<li class='item option-toggle'>
				<button class='add-module-btw'>
					<i class='fa fa-plus'></i>
				</button><br>
				<span class='invisible font_small'>Add</span>
			</li>
			<li class='option hidden item' data-module='half'>
				<button class='add-module-btw'>
					<i class='fa fa-file-server-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Half</span>
			</li>
			<li class='option hidden item' data-module='text'>
				<button class='add-module-btw'>
					<i class='fa fa-file-text-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Text</span>
			</li>
			<li class='option hidden item' data-module='video'>
				<button class='add-module-btw'>
					<i class='fa fa-file-video-o'></i>
				</button><br>
				<span class='invisible button-label font_small'>Video</span>
			</li>
		</ul>

		<section class='grid-module module padding-bottom padding-top'>
			<div class='row group wrapper'>
				<div class='six columns'>
					<i class='fa fa-server'></i>
					<span class='cursor-def'>Photo Grid</span>
				</div>
				<div class='six columns h-righted'>
					<i class='fa fa-remove a'></i>
				</div>
			</div>

			<div class='row group wrapper'>
				<article class='dropped-photo six columns'>
					<button class='remove-photo hidden'>
						<i class='fa fa-remove'></i>
					</button>
					<div class='img-container'><%= image_tag("project-chosen02.jpg", :alt => "") %></div>
					<textarea name='photo-caption' class='caption h-centered' placeholder='Add optional caption'></textarea>
					</input>
				</article>
				<article class='dropped-photo six columns'>
					<button class='remove-photo'>
						<i class='fa fa-remove'></i>
					</button>
					<div class='img-container'><%= image_tag("square.png", :alt => "") %></div>
					<textarea name='photo-caption' class='caption h-centered' placeholder='Add optional caption'></textarea>
					</input>
				</article>
			</div>

			<div class='row group wrapper'>
				<div class='droppable dropzone h-centered six columns'>
					<p class='caps'>A photo could be dropped here.</p>
				</div>
			</div>
		</section>

		<div class='button-bar'>
			<button class='add-module-end add-module'>
				<i class='h2-size fa fa-plus'></i>
			</button>
			<ul class='hidden'>
				<li class='item'>
					<button class='add-module-end add-module'>
						<i class='h2-size fa fa-file-photo-o'></i>
					</button><br>
					<span>Photo</span>
				</li>
				<li class='item'>
					<button class='add-module-end add-module'>
						<i class='h2-size fa fa-server'></i>
					</button><br>
					<span>Other</span>
				</li>
				<li class='item'>
					<button class='add-module-end add-module collapse'>
						<i class='h2-size fa fa-remove'></i>
					</button><br>
					<span id='esc'>Remove</span>
				</li>
				<li class='item'>
					<button class='add-module-end add-module'>
						<i class='h2-size fa fa-file-video-o'></i>
					</button><br>
					<span>Video</span>
				</li>
				<li class='item'>
					<button class='add-module-end add-module'>
						<i class='h2-size fa fa-file-text-o'></i>
					</button><br>
					<span>Text</span>
				</li>
			</ul>
		</div>


	</div>

</div>

