<% unless grids.nil? %>
  <% grids.each do | grid| %>
    <% if grid.id == mod[0].to_i %>


      <% if grid.photos.size > 0 && grid.photos.size == 1 %>

          <div id='grid-<%=grid.id%>' class='module-one wh-module uk-width-1-1 wh-height-viewport uk-grid uk-margin-bottom uk-margin-top'>
            <!-- Thumbnail -->
            <div class='wh-mod-photo-container wh-height-ninety uk-width-9-10 uk-grid-collapse uk-overlay uk-overlay-hover'>
              
              <figure class="uk-cover-background uk-width-1-1 wh-height-full" style="background-image:url(<%=grid.photos[0][0].photo_file%>)">
                <img class="uk-invisible" src="<%=grid.photos[0][0].photo_file%>" width="" height="" alt="<%=grid.photos[0][0].photo_file%>">
              </figure>

              <div class='uk-overlay-panel uk-overlay-slide-bottom uk-overlay-background uk-overlay-bottom'>
                <%= render partial: "maps/show_pieces/module_taglist", :locals => { :taglist => grid.taglist, :collaborators => collaborators } %>
                <%= render partial: "maps/show_pieces/module_lightbox_list", :locals => { :grid => grid, :lightbox_link_count => 0 } %>
                <p class='uk-float-left'>Bacon ipsum dolor amet beef ribs doner kevin meatball turkey meatloaf. Kielbasa venison ground round pork strip steak shoulder landjaeger meatloaf fatback corned beef andouille frankfurter. Turducken</p>
              </div>
            </div>
            <!-- Activity for all -->
            <div class='wh-mod-meta wh-height-ninety uk-width-1-10 uk-text-small uk-text-muted'>
              <p class='wh-italics'>
                Updated <br><%=grid.updated_at.strftime("%B %d, %Y") %>
              </p>
              <p>Comments will go here.</p>
            </div> 

          </div> <!-- end module -->


      <% elsif grid.photos.size > 0 && grid.photos.size == 2 %>
