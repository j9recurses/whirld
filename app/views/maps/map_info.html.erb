<div id='project-creation-2' class='row group project-creation'>
  


  <section id='photo-manager' class='side-nav four columns'>
    <%= render :partial => "maps/map_info_pieces/side" %>
  </section>
  <article class="temp-preloader four columns hidden">
    <h1 class='h3-size h-centered v-centered'>Uploading...</h1>
    <div class='progress-bar-wrapper'>
      <div class='progress-bar'></div>
    </div>
  </article>
  <div id='project-form' class='eight columns'>
    <section id='project-header'>
      <div class='wrapper'>

          <div class='row group'>
            <%= render :partial => "maps/map_info_pieces/form_title" %>
          </div>

          <div class='row group'>
            <%= render :partial => "maps/map_info_pieces/form_location" %>
          </div>

          <div class='row group'>
             <%= render :partial => "maps/map_info_pieces/form_description" %>
          </div>

          <div class='row group'>
           <%= render :partial => "maps/map_info_pieces/form_tags" %>
          </div>
          <section id="knitter-map-pane"></section>
      </div>
    </section> <!-- end #project-header -->

    <%= render :partial => "maps/map_info_pieces/buttonBar" %>
  </div> <!-- end #project-form -->
</div> <!-- end #project-creation -->