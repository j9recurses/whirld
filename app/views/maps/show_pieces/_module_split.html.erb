 <div id='split-<%=split.id%>' class='wh-module wh-mod-split uk-width-1-1 wh-height-viewport uk-grid uk-margin-bottom uk-margin-top'>
  <!-- Thumbnail -->
  <div class='wh-mod-wrapper wh-height-ninety uk-width-9-10 uk-grid-collapse'>

		<figure class="uk-cover-background wh-photo-half wh-photo-left uk-float-left wh-height-ninety uk-margin-bottom" style="background-image:url(<%=split.photos[0].photo_file%>)">
		  <img class="uk-invisible" src="<%=split.photos[0].photo_file%>" width="" height="" alt="<%=split.photos[0].photo_file%>">
		</figure>
  

		<div class='wh-photo-half wh-photo-right uk-float-right uk-text-break'>
			<p><%= split.split_text%></p>
		</div>
    <%= render partial: "maps/show_pieces/module_all_taglist", :locals => { :taglist => split.taglist, :collaborators => collaborators } %>
  </div>
  <!-- Activity for all -->
  <%= render partial: "maps/show_pieces/module_all_meta_side.html.erb", :locals => { :mod => split } %>
</div> <!-- end module -->